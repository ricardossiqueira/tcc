    "viewRule": null,
    "createRule": null,
    "updateRule": null,
    "deleteRule": null,
    "options": {
      "query": "SELECT \n    containers.id AS id, \n    COUNT(container_stats.id) AS count_requests, \n    AVG(COALESCE(NULLIF(container_stats.start_duration, 0), 0)) AS avg_start_duration, \n    AVG(COALESCE(NULLIF(container_stats.stop_duration, 0), 0)) AS avg_stop_duration \nFROM \n    container_stats\nJOIN\n    containers\nON\n    containers.id = container_stats.container\nGROUP BY\n    containers.id;\n"
    }
